{
  "version": 2,
  "builds": [
    {
      "src": "simple_main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "simple_main.py"
    }
  ],
  "env": {
    "ENVIRONMENT": "production",
    "DEBUG": "false"
  },
  "functions": {
    "simple_main.py": {
      "maxDuration": 30
    }
  }
}
