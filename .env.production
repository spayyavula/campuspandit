# Production Environment Variables
# These values will be used when building for production

# App Configuration
VITE_APP_TITLE="CampusPandit - Smart Learning Platform"
VITE_APP_VERSION="1.0.0"
VITE_APP_ENVIRONMENT="production"

# API Configuration
VITE_API_BASE_URL="https://api.campuspandit.com"
VITE_API_TIMEOUT="15000"

# Feature Flags
VITE_ENABLE_GAMING="true"
VITE_ENABLE_OFFLINE_MODE="true"
VITE_ENABLE_PWA_PROMPTS="true"

# External Services (Replace with actual production values)
VITE_ANALYTICS_ID="prod-analytics-id"
VITE_SENTRY_DSN="https://your-sentry-dsn@sentry.io/project-id"

# Board and Exam Configuration
VITE_SUPPORTED_BOARDS="cambridge,ib,cbse,isc,jee,neet"
VITE_DEFAULT_BOARD="cbse"

# Learning Platform Settings
VITE_MAX_LESSON_DURATION="60"
VITE_DEFAULT_SUBJECT="physics"
VITE_ENABLE_COMPETITIVE_EXAMS="true"